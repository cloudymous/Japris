<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/ListButtonHome"
    android:onClick="teleponKeluarga">

    <ImageView
        android:id="@+id/logoTelepon"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="16dp"
        android:src="@drawable/ic_phone_white_48dp" />

    <TextView
        android:id="@+id/menuTelepom"
        style="@style/regularTextStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:text="Telepon Keluarga"
        android:textSize="30sp" />

    <ImageView
        android:id="@+id/logoTelepon"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignParentEnd="true"
        android:layout_centerVertical="true"
        android:layout_marginEnd="16dp"
        android:src="@drawable/ic_chevron_right_white_48dp" />

</RelativeLayout>